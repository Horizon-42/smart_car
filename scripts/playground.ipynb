    "import cv2\n",
    "\n",
    "im = cv2.imread(\"test_data/road_transformed.jpg\")\n",
    "\n",
    "b,g,r = cv2.split(im)\n",
    "b2 = cv2.equalizeHist(b)\n",
    "g2 = cv2.equalizeHist(g)\n",
    "r2 = cv2.equalizeHist(r)\n",
    "\n",
    "im2 = cv2.merge([b, g, r])\n",
    "\n",
    "# im2 = cv2.equalizeHist(im)\n",
    "\n",
    "cv2.imwrite(\"im2.jpg\", im2)"
   ]
